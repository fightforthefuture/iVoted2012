<!DOCTYPE html>
<html>
<head>
  <title>iVoted2012</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body class="background_fade">
  <% if flash[:notice] %>
      <div id="notice"><span><p class="flash-notice"><%= flash[:notice] %></p></span></div>
  <% end %>
  <%= render :partial => "layouts/partials/menu"%>
  <table class="stars"><tr><td class="lft"></td><td><%= link_to image_tag("ivoted_logo.png"), "/" %></td><td class="rgt"></td></tr></table>
  <div id="page">
    <%= yield %>
  </div>
		<%= yield :footer %>
</body>
</html>
